{
	"name": "Link Visualizer",
	"description": "Visualizes links on this page!",
	"version": "1.5",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "img/Tree16.png",
		"48": "img/Tree48.png",
		"128": "img/Tree128.png"
	},
	"permissions": ["storage", "activeTab", "scripting"]
}